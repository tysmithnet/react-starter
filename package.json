{
  "name": "react-starter",
  "version": "2.0.0",
  "description": "My personal react starter project",
  "main": "index.js",
  "repository": "https://github.com/tysmithnet/react-starter",
  "author": "@tysmithnet",
  "license": "GPL-3.0-or-later",
  "private": false,
  "scripts": {
    "build": "webpack --color --mode production --config webpack.config.js",
    "db": "docker-compose -f server/docker-compose.yaml",
    "dep": "arkit",
    "doc": "typedoc --tsconfig tsconfig.json --out dist/docs/ --ignoreCompilerErrors src/ && http-server dist/docs",
    "e2e": "cypress open",
    "lint": "find src/ -type f -name *.ts -o -name *.tsx | xargs eslint",
    "lint:fix": "find src/ -type f -name *.ts -o -name *.tsx | xargs eslint --fix",
    "prebuild": "rimraf ./dist/*",
    "start": "node server/index.js",
    "storybook": "start-storybook",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@devexpress/dx-react-core": "^2.4.1",
    "@devexpress/dx-react-grid": "^2.4.1",
    "@devexpress/dx-react-grid-material-ui": "^2.4.1",
    "@material-ui/core": "4.9.3",
    "@material-ui/icons": "4.9.1",
    "@material/theme": "5.0.0",
    "axios": "0.19.2",
    "clsx": "1.1.0",
    "connected-react-router": "6.7.0",
    "history": "4.10.1",
    "lodash": "4.17.15",
    "luxon": "^1.22.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-saga": "1.1.3"
  },
  "devDependencies": {
    "@loadable/component": "^5.12.0",
    "@storybook/addon-actions": "5.3.12",
    "@storybook/addon-info": "5.3.12",
    "@storybook/addon-knobs": "5.3.12",
    "@storybook/addon-notes": "5.3.12",
    "@storybook/react": "5.3.12",
    "@types/enzyme": "3.10.5",
    "@types/jest": "25.1.2",
    "@types/lodash": "4.14.149",
    "@types/react": "^16.9.21",
    "@types/react-dom": "16.9.5",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "arkit": "1.6.2",
    "babel-loader": "8.0.6",
    "babel-preset-react-app": "9.1.1",
    "colors": "1.4.0",
    "connect-history-api-fallback": "1.6.0",
    "cypress": "4.0.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "express": "4.17.1",
    "fork-ts-checker-notifier-webpack-plugin": "2.0.0",
    "fork-ts-checker-webpack-plugin": "4.0.3",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.12.1",
    "jest": "25.1.0",
    "mysql": "2.18.1",
    "node-sass": "7.0.0",
    "prettier": "1.19.1",
    "react-docgen-typescript-loader": "3.6.0",
    "redux-devtools": "3.5.0",
    "rimraf": "3.0.2",
    "source-map-loader": "0.2.4",
    "ts-jest": "25.2.0",
    "ts-loader": "6.2.1",
    "tslib": "1.10.0",
    "typedoc": "0.16.9",
    "typescript": "3.7.5",
    "webpack": "4.6.0",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-middleware": "3.7.2",
    "webpack-dev-server": "3.10.3"
  }
}
